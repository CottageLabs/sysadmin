server {
    listen          80;
    server_name     test.oag.cottagelabs.com;

    access_log /var/log/nginx/test-oag-forward-to-PINKY.access.log;
    error_log /var/log/nginx/test-oag-forward-to-PINKY.error.log;

    location / {
        proxy_pass http://188.226.153.213;
        proxy_redirect off;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
	# proxy_buffering off;
    }
}

server {
    listen          80;
    server_name     flower.test.oag.cottagelabs.com;

    access_log /var/log/nginx/test-oag-forward-to-PINKY.access.log;
    error_log /var/log/nginx/test-oag-forward-to-PINKY.error.log;

    location / {
        proxy_pass http://188.226.153.213;
        proxy_redirect off;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }
}
